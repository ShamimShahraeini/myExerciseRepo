;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	www.digikala.com. root.digikala.com. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	www.digikala.com.
	IN	NS	www.digikala.com.
    IN    A    12.12.12.13
